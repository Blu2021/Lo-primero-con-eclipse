#include <stdio.h>
#include <stdlib.h>

int retornaEnteros(int enterin);

int main(void)
{
	setbuf(stdout,NULL);

	int enteroIngresado;
	int numeroRetornado;



	numeroRetornado=retornaEnteros(enteroIngresado);

	printf("el numero retornado es %d ",numeroRetornado);

	return EXIT_SUCCESS;
}

int retornaEnteros()
{
	int enterin;

	printf("Ingrese un numero entero\n");
	scanf("%d",&enterin);

	return enterin;
}
